{
  "framework": {
    "default": "netcoreapp3.1"
  },
  "projects": {
    "service": {
      //"path": "src\\services\\{SERVICE_NAME}",
      "path": "src",
      "template": "{COMPANY_NAME}.{SERVICE_NAME}.{PROJECT_TYPE}",
      "generate": true
    },
    "webapp": {
      "path": "src\\webapps\\{WEBAPP_NAME}",
      "template": "{COMPANY_NAME}.{WEBAPP_NAME}.{PROJECT_TYPE}",
      "generate": true
    },
    "unitTest": {
      "path": "test",
      "template": "{COMPANY_NAME}.{SERVICE_NAME}.{PROJECT_TYPE}",
      "generate": true
    },
    "client": {
      "path": "clients\\{SERVICE_NAME}",
      "template": "{COMPANY_NAME}.{SERVICE_NAME}.{PROJECT_TYPE}",
      "generate": true
    }
  }
}